use Array: all;
use StdIO: all;

noinline
int test()
{
    return sum(iota([1000,500,100]));
}

int main()
{
    rapl = Rapl::raplStart();
    printf("Found %d RAPL packages\n", shape(rapl)[0]);

    printf("res = %d\n", test());

    energy_uj = Rapl::raplStop(rapl);
    printf("%fJ\n", Rapl::tod(energy_uj));

    return 0;
}
