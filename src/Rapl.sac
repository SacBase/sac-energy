class Rapl;

external classtype;

export all;

objdef Rapl raplStart = rapl_init();

external Rapl rapl_init();
    #pragma linkobj "src/rapl.o"
    #pragma linksign [1]

external void rapl_start(Rapl &rapl);
    #pragma linkobj "src/rapl.o"

external void rapl_end(Rapl &rapl);
    #pragma linkobj "src/rapl.o"

void rapl_start()
{
    rapl_start(raplStart);
}

void rapl_end()
{
    rapl_end(raplStart);
}
